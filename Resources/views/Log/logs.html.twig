<script src="{{ asset('bundles/testbase/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>

<div class="container-fluid">
		<div class="page-header">
			<h1>Debug Log</h1>
		</div>
		<div class="row-fluid">
				<div class="well">
					<table class="table table-bordered table-striped">
							<tbody>
								<tr>
								  <td>MENSAJE</td>
								  <td>NIVEL</td>														 
								  <td>FECHA</td>							
								</tr>
								{% for message in debug %}
								<tr>
								  	<td>{{  message.message }}</td>
								 	<td>
							  			{% if message.context is not null %}
							  				<pre>
							  					{{ dump(message.context) }}
							  				</pre>	
							  			{% else %}
							  				Sin datos
										{% endif %}
									</td>
								
								  
								  {% if message.datetime is not null %}
								  <td>{{ message.datetime.date }}</td>
								  {% endif %}
								</tr>
								{% endfor %}
							</tbody>
					</table>	
				</div>
		</div>
</div>

